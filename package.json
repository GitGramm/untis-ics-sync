{
  "name": "units-ics-sync",
  "version": "0.0.1",
  "description": "Serves a calendar API (ICS) for events provided from Untis.",
  "main": "src/index.ts",
  "repository": "https://github.com/bddvlpr/untis-ics-sync.git",
  "author": "Luna Simons <luna@bddvlpr.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/index.ts",
